{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build:dev": "npm run build:development",
    "build:development": "env-cmd -f .env.development react-scripts build",
    "build:prod": "npm run build:production",
    "build:production": "env-cmd -f .env.production react-scripts build",
    "build:stg": "npm run build:staging",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "test:unit": "CI=true react-scripts test",
    "eject": "react-scripts eject"
  },
  "engines": {
    "node": "14.15.4",
    "yarn": "1.22.10"
  },
  "dependencies": {
    "@apollo/client": "3.1.3",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "@types/graphql": "14.5.0",
    "env-cmd": "10.1.0",
    "graphql": "15.3.0",
    "node-sass": "4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "typescript": "3.7.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@jest/globals": "24.9.0",
    "@types/jest": "24.9.1",
    "@types/node": "12.12.53",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "jest": "24.9.0",
    "prettier": "2.3.2",
    "prettier-plugin-organize-imports": "2.3.3"
  }
}
